<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" >
    <VBox>
        <Label text="Effectue la commande">
         <VBox.margin>
            <Insets left="130.0" top="10.0" />
         </VBox.margin></Label>

        <JFXComboBox fx:id="liste" focusColor="#22b199" labelFloat="true" prefWidth="200.0" promptText="commande">
         <VBox.margin>
            <Insets left="100.0" top="10.0" />
         </VBox.margin></JFXComboBox>
        <JFXButton buttonType="RAISED" onAction="#Effec" style="-fx-background-color: #16a085;" text="Faire une commande" textFill="WHITE">
            <font>
                <Font name="Calibri" size="15.0" />
            </font>
            <VBox.margin>
                <Insets bottom="20.0" left="120.0" top="40.0" />
            </VBox.margin></JFXButton>


    </VBox>

</AnchorPane>
